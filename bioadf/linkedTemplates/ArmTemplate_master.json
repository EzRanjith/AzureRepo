{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "bioadf"
		},
		"CustomerLinkservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'CustomerLinkservice'"
		},
		"azureADLSgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'azureADLSgen2'"
		},
		"bolbstoragelinkservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bolbstoragelinkservice'"
		},
		"CustomerLinkservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagetexh.dfs.core.windows.net/"
		},
		"azureADLSgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagetexh.dfs.core.windows.net/"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/5c1a0223-caf1-4698-ba01-c263f7e0f888/resourceGroups/biotech/providers/Microsoft.Storage/storageAccounts/storagetexh"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "bioadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"CustomerLinkservice_accountKey": {
						"value": "[parameters('CustomerLinkservice_accountKey')]"
					},
					"azureADLSgen2_accountKey": {
						"value": "[parameters('azureADLSgen2_accountKey')]"
					},
					"bolbstoragelinkservice_connectionString": {
						"value": "[parameters('bolbstoragelinkservice_connectionString')]"
					},
					"CustomerLinkservice_properties_typeProperties_url": {
						"value": "[parameters('CustomerLinkservice_properties_typeProperties_url')]"
					},
					"azureADLSgen2_properties_typeProperties_url": {
						"value": "[parameters('azureADLSgen2_properties_typeProperties_url')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}